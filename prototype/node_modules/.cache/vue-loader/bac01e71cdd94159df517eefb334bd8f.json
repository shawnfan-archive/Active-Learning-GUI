</v-layer>\n    </v-stage>\n    </div>\n    <button v-on:click=\"onSubmit\">Save</button>\n    <app-footer></app-footer>\n  </div>\n</template>>\n\n<script>\nimport axios from \"axios\";\nimport Header from \"./Header\";\nimport Footer from \"./Footer\";\n\nexport default {\n  data() {\n    return {\n      patients: {},\n      x: [],\n      y: [],\n      stageSize: {\n        width: 500,\n        height: 500\n      },\n      configLayer : {\n        visible: true\n      },\n      configImage: {\n        image: new Image()\n      }\n    };\n  },\n  methods: {\n    getPatients: function() {\n      const path = \"http://localhost:5000/active_learning\";\n      axios\n        .get(path)\n        .then(res => {\n          this.patients = res.data.patients;\n        })\n        .catch(error => {\n          console.error(error);\n        });\n    },\n    correctActivation: function(event) {\n      console.log(this.$refs.stage.getStage().getPointerPosition());\n      const pos = this.$refs.stage.getStage().getPointerPosition();\n      this.x.push(pos.x);\n      this.y.push(pos.y);\n    },\n    saveData: function(payload) {\n      const path = \"http://localhost:5000/active_learning\";\n      axios\n        .post(path, payload)\n        .then(() => {\n          alert(\"Progress saved!\");\n          this.getPatients();\n        })\n        .catch(error => {\n          console.log(error);\n          this.getPatients();\n        });\n    },\n    onSubmit: function(event) {\n      const payload = {\n        x_coord: this.x,\n        y_coord: this.y\n      };\n      this.saveData(payload);\n    }\n  },\n  components: {\n    \"app-header\": Header,\n    \"app-footer\": Footer\n  },\n  created() {\n    alert(\"Welcome to the Active Learning GUI!\");\n    this.getPatients();\n  },\n  mounted() {\n    this.configImage.image.src=\"https://www.radiologyinfo.org/gallery-items/images/picbrain.jpg\";\n  }\n};\n</script>\n\n<style scoped>\n#canvas {\n}\n</style>"]}]}